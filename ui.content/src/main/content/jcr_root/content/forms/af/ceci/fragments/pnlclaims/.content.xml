                        <fd:scripts
                            fd:click="[{&quot;script&quot;:{&quot;field&quot;:&quot;guide.guideRootPanel.btViewClaims&quot;\,&quot;event&quot;:&quot;Click&quot;\,&quot;model&quot;:{&quot;nodeName&quot;:&quot;EVENT_SCRIPTS&quot;}\,&quot;content&quot;:&quot;console.log(\\&quot;PnlClaims\\&quot;   + \\&quot;: \\&quot; + \\&quot;Guide Root Panel View Claims - Click\\&quot;\\t + \\&quot;: \\&quot; + \\&quot;Dec08A\\&quot; + \\&quot; \\&quot; + \\&quot;2022\\&quot;);\\ntestjs();\\nconsole.log(\\&quot;pnlclaims-Click 'View claims'\\&quot; + sessionStorage.getItem(\\&quot;fcprofileid\\&quot;));\\nthis.enabled = false; // do not allow user to click again\, creating a duplicate list of claims\\n\\nclaimsJsonObj = getClaims(vPreviousFcProfileId);\\nif (Array.isArray(claimsJsonObj)) {\\n    console.log(\\&quot;claimsJsonObj length =  \\&quot; + claimsJsonObj.length);\\n    //    pnlclaims.visible = true;\\n    guideRootPanel.section1.pnlLandingPage.pnlclaims.visible = true;\\n    guideRootPanel.section1.pnlLandingPage.pnlclaims.tblClaimsList.visible = true;\\n    guideRootPanel.section1.pnlLandingPage.pnlclaims.tblClaimsList.visible = true;\\n    //    guideRootPanel.section1.pnlLandingPage.pnlclaims.TitlePnl1.visible = true;\\n    guideRootPanel.section1.pnlLandingPage.pnlclaims.btSubmitClaim.visible = true;\\n    this.visible = false;\\n    txNoClaims.visible = false;\\n    var gac_name = claimsJsonObj[0].gac_name;\\n    console.log(\\&quot;claimsJsonObj gac_name =  \\&quot; + gac_name);\\n\\n    var destObj = guideRootPanel.section1.pnlLandingPage.pnlclaims.tblClaimsList.Row1;\\n    console.log(\\&quot;destObj.instanceManager.instances[0].iclaimtitle =  \\&quot; + destObj.instanceManager.instances[0].iclaimtitle.value);\\n    var strDate = \\&quot;\\&quot;;\\n\\n  for (var i = 0; i &lt; claimsJsonObj.length; i++) {\\n        console.log(\\&quot;i: \\&quot; + i);\\n        if (i > 0) destObj.instanceManager.insertInstance();\\n        var buttonValues = getClaimButtonLabelEnable(claimsJsonObj[i].statuscode);\\n    //buttonValues is an array\, [0] item is the 'Status column' text\\n    //                          [1] item determines the lable for the 'Edit'Details' button\\n        var statusColumnText = 0;\\n        var buttonEnabled = 1;\\n        var vButtonLabel = \\&quot;\\&quot;;\\n        destObj.instanceManager.instances[i].btGotoClaims.title = buttonValues[statusColumnText];\\n        //    destObj.instanceManager.instances[i].btDetails.enabled = buttonValues[1];\\n\\n        if ((buttonValues[0] != \\&quot;Processed\\&quot;) &amp;&amp; (buttonValues[0] != \\&quot;traité\\&quot;)) {\\n            btSubmitClaim.enabled = false;\\n            txPleaseNoteClaim.visible = true;\\n        }\\n        if (buttonValues[buttonEnabled]) {\\n            vButtonLabel = vLang === \\&quot;en\\&quot; ? \\&quot;Edit\\&quot; : \\&quot;Modifier\\&quot;;\\n            destObj.instanceManager.instances[i].btGotoClaims.title = vButtonLabel;\\n            btSubmitClaim.enabled = false;\\n            txPleaseNoteClaim.visible = true;\\n        } else {\\n          vButtonLabel = vLang === \\&quot;en\\&quot; ? \\&quot;Details\\&quot; : \\&quot;Détails\\&quot;;\\n          destObj.instanceManager.instances[i].btGotoClaims.title = vButtonLabel;\\n          destObj.instanceManager.instances[i].btGotoClaims.enabled = true;  //Besco-Jun13-allow user to click button and open Claims WITH r/o fields\\n        }\\n\\n        destObj.instanceManager.instances[i].iclaimtitle.value = claimsJsonObj[i].gac_name;\\n        destObj.instanceManager.instances[i].iclaimnum.value = claimsJsonObj[i].gac_claimnumber;\\n        destObj.instanceManager.instances[i].iclaimamount.value = new Intl.NumberFormat('en-CA'\, {\\n            style: 'currency'\,\\n            currency: 'CAD'\\n        }).format(claimsJsonObj[i].gac_totalapprovedpayment);\\n\\n        // when a claim is in \\&quot;draft\\&quot;\, gac_datesubmitted will be null\, otherwise\, it is a valid date object\, so split off time. besco\\n        strDate = claimsJsonObj[i].gac_datesubmitted;\\n        if (strDate === null) destObj.instanceManager.instances[i].iclaimdate.value = \\&quot;\\&quot;;\\n        else destObj.instanceManager.instances[i].iclaimdate.value = strDate.split(\\&quot;T\\&quot;)[0];\\n\\n        //hidden values below are required for passing to Claims_Form via sessionstore\\n        destObj.instanceManager.instances[i].gac_fdiclaimid.value = claimsJsonObj[i].gac_fdiclaimid;\\n        destObj.instanceManager.instances[i].strStatus.value = buttonValues[statusColumnText]; // 'buttonValues' array item [0]\\n\\n        console.log(\\&quot;claimsJsonObj[i].statuscode =  \\&quot; + claimsJsonObj[i].statuscode);\\n    }\\n} else {\\n    if (claimsJsonObj.result.toLowerCase() === \\&quot;No response\\&quot;.toLowerCase()) {\\n        console.log(\\&quot;No Claims\\&quot;);\\n        txNoClaims.visible = true;\\n        //        pnlclaims.visible = false;\\n        tblClaimsList.visible = false;\\n        guideRootPanel.section1.pnlLandingPage.pnlclaims.btSubmitClaim.visible = true;\\n    }\\n}\\nconsole.log(\\&quot;******execCRMServiceAjax_ceciclaim - Click - DONE\\&quot;);&quot;}\,&quot;nodeName&quot;:&quot;SCRIPTMODEL&quot;\,&quot;version&quot;:1\,&quot;enabled&quot;:true}]"
