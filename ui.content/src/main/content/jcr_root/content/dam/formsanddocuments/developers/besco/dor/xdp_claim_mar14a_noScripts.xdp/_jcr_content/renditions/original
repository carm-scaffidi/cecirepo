      <variables>
         <script contentType="application/x-javascript" name="scoClaims">/************************************************************************************************* 
* When form loads set the variable vXML with the Components data premerged into the form and create an e4X object
**************************************************************************************************/
var vXML = xfa.datasets.data.ICCIClaimData.Components.saveXML("pretty");
//Create an E4X object			
var vXMLDoc = new XML(scoCommon.trimXML(vXML));

/************************************************************************************************* 
* Called from the subform containing the repeatable checkbox on form:ready
* Takes the data, counts the number of components and creates that many instances of the checkbox
* Then takes the Title and Component Id for each component, concatonates them and assigns them as the checkbox caption
**************************************************************************************************/
function setClaimChecks(){
	if (vXML != null){
		var vLength = vXMLDoc.Component.length();
		var vCaption;
		xfa.resolveNode("pageSelectComponents._subComponent").setInstances(vLength);
		for (i=0;i&lt;vLength;i++){
			vCaption = "";
			vCaption = vXMLDoc.Component[i].Title.toString() + " - " + vXMLDoc.Component[i].ComponentNumber.toString();
			xfa.resolveNode("pageSelectComponents.subComponent[" + i + "].chkComponent").caption.value.text.value = vCaption;
		}
	}
}


/*************************************************************************************************
* Used to populated drop down lists within a component with the values available based upon the 
* Activity Category (material group) set for that component.
* could not use dynamic binding on because would only  bring in values from first component as 
* there is no way to differentiate in the property settings which component instance to take the 
* values from
* Input: oDDL is the drop down list object to populate
* Input: nodeGroup is the xml node name of the group (e.g. ExpenseTypes or Units)
* Input: componentNo is the component number whose Expense Types or Units you want to populate
**************************************************************************************************/
function setDDL(oDDL, nodeGroup, componentNo){
	//clear the list to start
	oDDL.clearItems()
	//only do this if there are values to populate
	if (vXML != null){		
		var vValue = vXMLDoc.Component.(ComponentNumber==componentNo);
		if (nodeGroup == "ExpenseTypes"){
			//cycle through the returned values and assign them to the DDList
			for (i=0;i&lt; vValue.ActivityExpenseTypes.ActivityExpenseType.length();i++){
				oDDL.addItem(vValue.ActivityExpenseTypes.ActivityExpenseType[i].DescriptionEnglish.toString(), vValue.ActivityExpenseTypes.ActivityExpenseType[i].ID.toString())
			}
		}
		else if (nodeGroup == "Units"){
			//cycle through the returned values and assign them to the DDList
			for (i=0;i&lt; vValue.UnitOfMeasureTypes.UnitOfMeasureType.length();i++){
				oDDL.addItem(vValue.UnitOfMeasureTypes.UnitOfMeasureType[i].DescriptionEnglish.toString(), vValue.UnitOfMeasureTypes.UnitOfMeasureType[i].ID.toString())
			}
		}
	}
}


/*************************************************************************************************
* Called from the change event of the claim checkbox to either show a component instance or to hide 
* the component instance associated with the claim checkbox. Hiding should make it inacive so it is
* not mandatory.
* Input: oFld - this is the claim checkbox object
**************************************************************************************************/
function setClaimComponent(oFld, iInstance){
	var oClaimComponents = xfa.resolveNodes("pageComponent[*]");	
	//If this is checked then show the component. If unchecked then hide it and clear any data from user driven fields and reset component claim flag.
	if (oFld.rawValue == 1){
			oClaimComponents.item(iInstance).presence = "visible";
			oClaimComponents.item(iInstance).ctrlchkClaimedAgainst.rawValue = 1;
			//These three lines are needed to trigger the calculation to register in memory for components that move from inactive to visible
			oClaimComponents.item(iInstance).subExpenses.tblExpenses.Row1.decExpenseTotalBeforeTaxes.execCalculate();
			oClaimComponents.item(iInstance).subComponentClaimSummary.decTotalCost.execCalculate();
			oClaimComponents.item(iInstance).subComponentClaimSummary.decOverallAmountToBePaid.execCalculate();
	}
	else {
		var iDecision = xfa.host.messageBox("Unchecking this box will remove any data entered for this component.", "Warning", 1, 1);
		if (iDecision == "1"){
			var vCompId = oClaimComponents.item(iInstance).subSpacer.subComponentSummary.decComponentNo.rawValue;
			scoCommon.resetTable(oClaimComponents.item(iInstance).subExpenses.tblExpenses);
			oClaimComponents.item(iInstance).subExpenses.tblExpenses.Row1.numActivityExpenseNumber.rawValue = 1; //reset row number to 1
			oClaimComponents.item(iInstance).subExpenses.tblExpenses.Row1.decExpenseTotalBeforeTaxes.rawValue = 0.00; //reset line item total for row 1
			oClaimComponents.item(iInstance).subComponentClaimSummary.decTotalCost.rawValue = 0.00; //reset total of expense rows
			oClaimComponents.item(iInstance).subComponentClaimSummary.decOverallAmountToBePaid.rawValue = 0.00; //reset total requested to be paid
			oClaimComponents.item(iInstance).subComponentClaimQuestions.subComponentComplete.rblComponentComplete.rawValue = 0; //reset radio button
			oClaimComponents.item(iInstance).subComponentClaimQuestions.fldProgressVariance.rawValue = null; //reset Progress variance field
			xfa.resolveNode(oClaimComponents.item(iInstance).subExpenses.tblExpenses.somExpression)._Row1.setInstances(1); //reset table to start with only 1 row
			oClaimComponents.item(iInstance).presence = "inactive";
			oClaimComponents.item(iInstance).ctrlchkClaimedAgainst.rawValue = 0;
		}
		else{
			oFld.rawValue = 1;
		}
	}
}


/*************************************************************************************
Function: SetMandatoryMessage
Description: This function will set the mandatory messages for visible fields.
IN: The parent subform. It could also be an element that contains subform like form1
OUT : nothing
**************************************************************************************/
function setMandatoryMessage(myParentObject){

	var allChildElements;
	var intNumElements;
	var currentElement;
	var i,j,k;
	var mandatoryMsg;

	
	//Get all the child nodes of the parent element
	allChildElements = myParentObject.nodes;

	//Total number of element in the object
	intNumElements = allChildElements.length;
	
	//Loop through all the child elements
	for(j=0; j&lt; intNumElements;j++){
		currentElement = allChildElements.item(j);
		//If the element is another subform we'll recusively call the function again
		if(allChildElements.item(j).className == "subform"){
			setMandatoryMessage(currentElement);
		}
		//If the objects are fields and they are set to mandatory (validate.nullTest) then we will set the border.fill.color - dependant on object type
		else if(currentElement.className == "field" &amp;&amp; currentElement.className !== "button"){			
				if (currentElement.presence == "visible") {
						//currentElement.mandatory = "error";
						if (currentElement.mandatoryMessage != null) {
							//Truncate the message shown to the user in the error list box.
							if (currentElement.mandatoryMessage.substring(0,23) == "You must enter a value:"){
								currentElement.nodes.remove(currentElement.validate);
							}
							if (currentElement.mandatoryMessage.length &gt; 145) {
								currentElement.mandatoryMessage = currentElement.mandatoryMessage.substring(0,145) + "...";
							}
								currentElement.mandatoryMessage =  "You must enter a value: " + currentElement.mandatoryMessage; //currentElement.caption.value.text.value + " is required."
						}
						else {
							currentElement.mandatoryMessage =  "You must enter a value: " + currentElement.assist.speak.value;
						}							
				}			
			}			
			//Check for exclusion groups - Radio Buttons
			else if(currentElement.className == "exclGroup"){
				for(k=0; k&lt; currentElement.nodes.length;k++){
					if(currentElement.nodes.item(k).className == "field"){
						//if(currentElement.presence == "visible"){
						if(currentElement.mandatoryMessage != null){
							if (currentElement.mandatoryMessage.substring(0,23) == "You must enter a value:"){
								currentElement.nodes.remove(currentElement.validate);
							}
							if (currentElement.mandatoryMessage.length &gt; 145) {
								currentElement.mandatoryMessage = currentElement.mandatoryMessage.substring(0,145) + "...";
							}
							currentElement.mandatoryMessage =  "You must enter a value: " + currentElement.mandatoryMessage;
						}
					}
				}
			}	
		}
	
}//end function


/*************************************************************************************
Function: toggleMandatory
Description: This function will set whether a field is mandatory or not. It is used to
workaround the Adobe bug that does not ignore InvalidObjects within Inactive subforms.
Specifically related to if yuo fill a component (and have an error), then validate, then
decide to remove the component (make inactive) the validation does work in that it ignores
the fields in the inactive but the getInvalidObjects method does not ignore them so the
error listbox does not work properly.  This is logged with Adobe and it is too late to 
change the architecture to manually control the whole form through code with respect to
mandatory and non-mandatory fields.
IN: The parent subform. It could also be an element that contains subform like form1
IN: A boolean representing if the field should be toggled to mandatory or open
OUT : nothing
**************************************************************************************/
function toggleMandatory(myParentObject, bMandatory){

	var allChildElements;
	var intNumElements;
	var currentElement;
	var i,j,k;
	var vMandatory;
	
	if (bMandatory){
		vMandatory = "error";
	}
	else {
		vMandatory = "disabled";
	}

	
	//Get all the child nodes of the parent element
	allChildElements = myParentObject.nodes;

	//Total number of element in the object
	intNumElements = allChildElements.length;
	
	//Loop through all the child elements
	for(j=0; j&lt; intNumElements;j++){
		currentElement = allChildElements.item(j);
		//If the element is another subform we'll recusively call the function again
		if(allChildElements.item(j).className == "subform"){
			toggleMandatory(currentElement);
		}
		//If the objects are fields then toggle their mandatory property
		else if(currentElement.className == "field" &amp;&amp; currentElement.className !== "button"){			
			currentElement.mandatory = vMandatory;				
		}			
			//Check for exclusion groups - Radio Buttons
			else if(currentElement.className == "exclGroup"){
				for(k=0; k&lt; currentElement.nodes.length;k++){
					if(currentElement.nodes.item(k).className == "field"){
						currentElement.mandatory = vMandatory;				
					}
				}
			}	
		}
	
}//end function
</script>
         <script contentType="application/x-javascript" name="scoCommon">/************************************************************************************************* 
* Utility function triming an XML string before it can be used to create an E4X JavaScript object
**************************************************************************************************/
function trimXML(sXML)
{
	// Need a string to continue.
	if (sXML == null)
		return sXML;
		
	// Remove everything prior to the opening element
	sXML = sXML.replace(/^[\s\S]*?(&lt;[^\?!])/, "$1");

	// Remove everything after 
	return sXML.replace(/(&gt;)[\s]*?$/, "$1");
}


/** 
 * Utility function reseting the children of the given subform. Isn't recursive at this time. 
 * @param oSubform is a reference to the subform holding the fields to reset.
 */
function resetFields(oSubform)
{
	// Don't use resetData since it triggers the form validation, twice!
	// We'll avoid calling setInstances as well as it seems to affect
	// performance.
	var oFields = oSubform.resolveNodes("#field[*]");
	var nLen = oFields.length;
	for (var i = 0; i &lt; nLen; i++){
		oFields.item(i).rawValue = null;
	}
}

/** 
 * Utility function reseting the children of the given subform. Isn't recursive at this time. 
 * @param oSubform is a reference to the subform holding the fields to reset.
 */
function resetTable(oTable)
{
	// Don't use resetData since it triggers the form validation, twice!
	// We'll avoid calling setInstances as well as it seems to affect
	// performance.
	var oRows = oTable.resolveNodes("#subform[*]");
	var nLen = oRows.length;
	for (var i = 0; i &lt; nLen; i++){
		var oRowItems = oRows.item(i).resolveNodes("#field[*]");
		var nItems = oRowItems.length;
		for (var j=0; j &lt; nItems; j++){
			oRowItems.item(j).rawValue = null;
		}
	}
}


/*************************************************************************************
* Function: LockAllFields
* Description: This function will lock all fields.
* IN: The parent subform. It could also be an element that contains subform like form1
* OUT : nothing
**************************************************************************************/
function LockAllFields(myParentObject){

	var allChildElements;
	var intNumElements;
	var currentElement;
	var j,k;
	
	//Get all the child nodes of the parent element
	allChildElements = myParentObject.nodes;

	//Total number of element in the object
	intNumElements = allChildElements.length;
	
	//Loop through all the child elements
	for(j=0; j&lt; intNumElements;j++){
		currentElement = allChildElements.item(j);
		//If the element is another subform we'll recusively call the function again
		if(allChildElements.item(j).className == "subform"){
			LockAllFields(currentElement);
		}
		//If the objects are fields then we will set them to read only
		else if(currentElement.className == "field"){
			//if(currentElement.name.substring(0,3) != "btn"){
				currentElement.access = "readOnly";
			//}
		}
			//Check for exclusion groups - Radio Buttons
			else if(currentElement.className == "exclGroup"){
			for(k=0; k&lt; currentElement.nodes.length;k++){
				if(currentElement.nodes.item(k).className == "field"){
					//set the radio button to read only
					currentElement.access = "readOnly";
				}
			
			}
		}	
	}
}//end function

/*************************************************************************************
* Function: OpenFields
* Description: This function will lock all fields.
* IN: The parent subform. It could also be an element that contains subform like form1
* OUT : nothing
**************************************************************************************/
function OpenFields(myParentObject){

	var allChildElements;
	var intNumElements;
	var currentElement;
	var j,k,m;
	
	//This code will be used to bypass fields that should not be opened. 
	var fieldString = "numActivityExpenseNumber,decExpenseTotalBeforeTaxes,decProjApproveAmt,decProjPaidAmt,decProjRemainingAmt,fldAmountPaidSummary,fldTotalFundingApproved"
	var fieldArray = new Array();
	fieldArray = fieldString.split(",")
	
	//Get all the child nodes of the parent element
	allChildElements = myParentObject.nodes;

	//Total number of element in the object
	intNumElements = allChildElements.length;
	
	//Loop through all the child elements
	for(j=0; j&lt; intNumElements;j++){
		currentElement = allChildElements.item(j);
		//If the element is another subform we'll recusively call the function again
		if(allChildElements.item(j).className == "subform"){
			OpenFields(currentElement);
		}
		//If the objects are fields and they are set to mandatory (validate.nullTest) then we will set the border.fill.color - dependant on object type
		else if(currentElement.className == "field"){
			//if(currentElement.name.substring(0,3) != "btn"){
			currentElement.access = "";
			//}
			//The fields in teh fieldArray must remain as read only so put these back to read only
			for (m=0;m&lt;fieldArray.length;m++){
				if (currentElement.name == fieldArray[m]){
					currentElement.access = "readOnly";
				}
			}
		}
		//Check for exclusion groups - Radio Buttons
		else if(currentElement.className == "exclGroup"){
		for(k=0; k&lt; currentElement.nodes.length;k++){
			if(currentElement.nodes.item(k).className == "field"){
				//set the color for the radio buttons individually
				currentElement.access = "";
			}
		}
		}	
	}
}//end function
</script>
         <script contentType="application/x-javascript" name="scoValidations">/*************************************************************************************
* Function - Trim leading and trailing spaces of given value.
* Description - used to get rid of spaces in a value to allow for proper comparison and manipulation
* IN: sValue the string to remove leading and trailing spaces.
* OUT: the value inputted minus any leading or trailing spaces
**************************************************************************************/
function trimSpaces(sValue)
{
	if (sValue == null)
		return sValue;
		
	return sValue.replace(/^\s+|\s+$/g, "");	
}


/*************************************************************************************
* Function - validates the value meets the appropriate email format.
* IN: oField. The value of email address to validate.
* OUT: Returns true when valid, false otherwise.
**************************************************************************************/
function validateEmailFormat(oField)
{
	if (validateNow.value == "true") {
	//if (oField.isNull) 
	//	return true;
	
	// Trim leading and trailing spaces as they are not supported by all email servers.
	// Done here but will move at the object level when we implement the wcag validation.
	// Also, when placed on the exit event, the validation fires before and fails before
	// we have time to process the value.
	var sToTrim = oField.rawValue;
	oField.rawValue = trimSpaces(sToTrim);

	// Validate the email address.
	var r = new RegExp("^[a-z0-9_\\-\\.]+\\@[a-z0-9_\\-\\.]+\\.[a-z]{2,3}$");
	return r.test(oField.rawValue.toLowerCase());
	}
	else {
		return true;
	}
}

/*************************************************************************************
* Function - validates the value meets the appropriate phone number format.
* IN: oField. The value of phone number to validate.
* OUT: Returns true when valid, false otherwise.
**************************************************************************************/
function validatePhoneNumberFormat(oField)
{
	if (validateNow.value == "true") {	
	// Trim leading and trailing spaces as they are not supported by all email servers.
	// Done here but will move at the object level when we implement the wcag validation.
	// Also, when placed on the exit event, the validation fires before and fails before
	// we have time to process the value.
	var sToTrim = oField.rawValue;
	oField.rawValue = trimSpaces(sToTrim);

	// Validate the email address.
	var r = new RegExp("^[0-9]{10}");
	return r.test(oField.rawValue);
	}
	else {
		return true;
	}
}



/*************************************************************************************
* Function - Sets the display message to show for any field with a maximum character restriction.
**************************************************************************************/
function maxCharMessage() {
	xfa.host.messageBox("You have reached the maximum characters for this field.", "Warning", 1, 0);
}



/*************************************************************************************
* Function - Enforces the minimum number of characters for a field and sets the display message
* IN: oFld field to validate.
* IN: pMinLength is the number of characters to validate as minimum length for the field passed in.
* OUT: Returns either true or false as this function is called from the validation event of a field.
**************************************************************************************/
function minCharValidation(oFld, pMinLength){
	if (validateNow.value == "true") {
		//If null just return false as we have set the empty field message right in the properties of the field
		if (oFld.isNull){
				return false;
			}
		// Enforce minimum character restriction
		else if (oFld.rawValue.length &lt; pMinLength) {
			oFld.validationMessage = "You must have at least " + pMinLength + " characters for this field. You currently only have " + oFld.rawValue.length + " characters.";
			return false;
		}
		else {
			return true;
		}
	}
	else {
		return true;
	}
}


/*************************************************************************************
* Function - Setup error list box with fields that have an error.
* Description - Part of the code necessary to validate the form. 
* DataRoot node has some code to set this up and the rest is on the error ListBox object.
* the idea is not to identify the error listbox until 
* document ready so that we don't re-populate it with every
* validationState event on form open
**************************************************************************************/
var errListField = null;
function setErrorListBox(obj) {
	errListField = obj;
}
function buildErrorListBox() {
	if (errListField === null) {
		app.alert("no error list box exists.");
		return;
	}	
	// get the complete set of errors from the root subform
	//var list = xfa.form["#subform"].getInvalidObjects();
	var list = xfa.form["ICCIClaimData"].getInvalidObjects();
	
	// repopulate the listbox with the current set of errors.
	var listContent = [];
	// use the error message as the displayed value, and use the 
	// som expression as the save value.
	for (var i = 0; i &lt; list.length; i++) {
		listContent.push(list.item(i).errorText);
		listContent.push(list.item(i).somExpression);
	}
	errListField.setItems(listContent.join(","), 2);
}

</script>
