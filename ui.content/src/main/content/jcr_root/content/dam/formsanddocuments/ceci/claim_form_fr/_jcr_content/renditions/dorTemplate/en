            <script contentType="application/x-javascript" name="dorRadioButtonMerge">
            function mergeData(field, textField) {
                if(dorMergeElementOverriddenData.isFieldOverridden(field.parent)){
                    return;
               }
               var radioButtonList = textField.parent.resolveNode("AF_RADIOBUTTONLIST");
               var hiddenField = textField.parent.resolveNode(textField.parent.name);
               var oNodes = radioButtonList.nodes || [];
               var nNodesLength = oNodes.length;
               var sValue = getValueFromHiddenTextField(hiddenField);
               var oOption = null;

               if (dorShowSelectedOptions.value === "true") {
                    hideRadioButtons(radioButtonList);
                    if (hiddenField.rawValue != null){
                        oOption = getSelectedNode(radioButtonList, sValue);
                    }
               } else {
                    radioButtonList.rawValue = sValue;
               }

               // if radio button is inside table radio button title text field will be hidden
               // make it visible
               textField.presence = "visible";

               // assign caption of selected option as value of textField
               if ( oOption != null &amp;&amp; dorShowSelectedOptions.value === "true" ) {
                    textField.rawValue = oOption.caption.value.text.value;
               }
            }

            function hideRadioButtons(radioButtonList) {
                var oNodes = radioButtonList.nodes || [];
                var nNodesLength = oNodes.length;
                 for (var nNodeCount = 0; nNodeCount &lt; nNodesLength; nNodeCount++) {
                 var oNode = oNodes.item(nNodeCount);
                 if ( oNode.className === "field" ) {
                    oNode.presence = "hidden";
                 }
               }
            }

            function getSelectedNode(radioButtonList, radioButtonListValue) {
                var oNodes = radioButtonList.nodes || [];
                var nNodesLength = oNodes.length;
                var oOption = null;
                for (var nNodeCount = 0; nNodeCount &lt; nNodesLength; nNodeCount++) {
                  var oNode = oNodes.item(nNodeCount);
                  if ( oNode.className === "field" ) {
                     var oItemsValue = oNode.resolveNode("#items.#text") || oNode.resolveNode("#items.#integer");
                     if ( oItemsValue != null &amp;&amp; oItemsValue.value === radioButtonListValue) {
                        oOption = oNode;
                        break;
                     }
                  }
               }
               return oOption;
            }

         function getValueFromHiddenTextField(hiddenTextField) {
            return hiddenTextField.rawValue != null ? hiddenTextField.rawValue.toString().replace(/^\s+|\s+$/g,'') : "";
         }

         function overrideOptionsForRadioButton(field, fieldOverriddenData) {
                var somExpr = field.somExpression.substring(xfa.form.data.somExpression.length + 1);
                var elementOverrideData = fieldOverriddenData[somExpr];
                if (elementOverrideData.hasOwnProperty("options")) {
                    var hiddenTextField = field.resolveNode(field.name);
                    var titleField = field.resolveNode("AF_RADIOBUTTONLISTTITLE");
                    var radioButtonList = field.resolveNode("AF_RADIOBUTTONLIST");
                    titleField.presence = "visible";
                    var optionsMap = elementOverrideData.options;
                    var radioButtonListValue = getValueFromHiddenTextField(hiddenTextField);
                    hideRadioButtons(radioButtonList);
                    if ( radioButtonListValue != null &amp;&amp; optionsMap.hasOwnProperty(radioButtonListValue) ) {
                        titleField.rawValue = optionsMap[radioButtonListValue];
                    }
                }
            }
</script>
            <text name="dorValueSeparator">, </text>
            <text name="dorShowSelectedOptions">true</text>
            <text name="dorRowShadingInst">templateDesigner rowpattern first:.*, next:.*, firstcolor:.*, nextcolor:.*, apply:1</text>
            <script contentType="application/x-javascript" name="dorCheckBoxMerge">
            function mergeData(field, textField) {
               if(dorMergeElementOverriddenData.isFieldOverridden(field.parent)){
                    return;
               }
               var values = field.rawValue != null ? field.rawValue.toString().split(",") : [];
               var checkBoxValues = getCheckBoxValues(field.parent);

               // if check box is inside table check box title text field will be hidden
               // make it visible and hide check boxes
               textField.presence = "visible";
               if (dorShowSelectedOptions.value === "true") {
                  hideCheckBoxes(field.parent);
               } else {
                  resetCheckBoxes(field.parent);
               }

               // if no option selected hide check box subform
               if ( values != null ) {
                  var displayValues = [];
                  var lengthOfValues = values.length;
                  for (var count = 0; count &lt; lengthOfValues; count++) {
                      // get check box button corresponding to value
                      var value = values[count] || "";
                      var checkBoxButton = checkBoxValues[value.toString().replace(/^\s+|\s+$/g, '')];
                      if ( checkBoxButton != null &amp;&amp; checkBoxButton.className === "field") {
                         if (dorShowSelectedOptions.value === "true") {
                            // push display into displayValues array
                            displayValues.push(checkBoxButton.caption.value.text.value);
                         } else {
                            checkBoxButton.value.integer = 1;
                         }
                      }
                  }
                  textField.rawValue = displayValues.join(dorValueSeparator.value) || "";
               }
            }

            function hideCheckBoxes(nodeParent) {
               var oNodes = nodeParent.nodes || [];
               var nNodesLength = oNodes.length;

               // hide all check boxes as selected options are shown together in textField
               for (var nNodeCount = 0; nNodeCount &lt; nNodesLength; nNodeCount++) {
                  var oField = oNodes.item(nNodeCount);
                  if ( oField.className === "field" )
                     if ( oField.resolveNode("#ui.#checkButton") != null ) {
                        oField.presence = "hidden";
                     }
               }
            }

            function resetCheckBoxes(nodeParent) {
               var oNodes = nodeParent.nodes || [];
               var nNodesLength = oNodes.length;

               // hide all check boxes as selected options are shown together in textField
               for (var nNodeCount = 0; nNodeCount &lt; nNodesLength; nNodeCount++) {
                  var oField = oNodes.item(nNodeCount);
                  if ( oField.className === "field" ) {
                     if ( oField.resolveNode("#ui.#checkButton") != null ) {
                        oField.value.integer = 0;
                     }
                  }
               }
            }

            function getCheckBoxValues(nodeParent) {
               var mValues = {};
               var oNodes = nodeParent.nodes || [];
               var nNodesLength = oNodes.length;

               // collect save values against check box in a map
               for (var nNodeCount = 0; nNodeCount &lt; nNodesLength; nNodeCount++) {
                  var oField = oNodes.item(nNodeCount);
                  if ( oField.className === "field" ) {
                     if ( oField.resolveNode("#ui.#checkButton") != null ) {
                        sSaveValue = oField.resolveNode("extras.saveValue") || null;
                        if ( sSaveValue != null ) {
                           mValues[sSaveValue.value.toString().replace(/^\s+|\s+$/g, '')] = oField;
                        }
                     }
                  }
               }

               return mValues;
            }
            function overrideOptionsForCheckBox(field,fieldOverriddenData) {
                var somExpr = field.somExpression.substring(xfa.form.data.somExpression.length + 1);
                var elementOverrideData = fieldOverriddenData[somExpr];
                if (elementOverrideData.hasOwnProperty("options")) {
                    var hiddenTextField = field.resolveNode(field.name);
                    field.resolveNode("AF_CHECKBOXTITLE").presence = "visible";
                    var optionsMap = elementOverrideData.options;
                    var values = hiddenTextField.rawValue != null ? hiddenTextField.rawValue.toString().split(",") : [];
                    hideCheckBoxes(field);
                    if ( values != null ) {
                        var displayValues = [];
                        var lengthOfValues = values.length;
                        for (var count = 0; count &lt; lengthOfValues; count++) {
                            var value = values[count] || "";
                            if (optionsMap.hasOwnProperty(value)) {
                                displayValues.push(optionsMap[value]);
                            }
                        }
                        }
                    field.resolveNode("AF_CHECKBOXTITLE").rawValue = displayValues.join(dorValueSeparator.value) || "";
                }
            }

            function overrideOptionsForImageChoice(field,fieldOverriddenData) {
                var somExpr = field.somExpression.substring(xfa.form.data.somExpression.length + 1);
                var elementOverrideData = fieldOverriddenData[somExpr];
                if (elementOverrideData.hasOwnProperty("options")) {
                    var hiddenTextField = field.resolveNode(field.name);
                    field.resolveNode("AF_IMAGECHOICETITLE").presence = "visible";
                    var optionsMap = elementOverrideData.options;
                    var values = hiddenTextField.rawValue != null ? hiddenTextField.rawValue.toString().split(",") : [];
                    hideCheckBoxes(field);
                    field.resolveNode("AF_IMAGECHOICETITLE").rawValue = hiddenTextField.rawValue || "";
                }
            }
</script>
            <script contentType="application/x-javascript" name="dorExcludeFromDoR">
            function exclude(oSOMExprs) {
                    var nIndex = -1;
                    var nPrevIndex = -1;
                    var oFields = {};
                    var oSeparator = '=';
                    var oTrueStatus = "true";

                    var oFieldStatus;
                    var nIndexOfSeparator;
                    var oSOMExpr;
                    var bStatus;
                    var oStatus;

                    // initialize oFields map with templateIds or som expressions and their dorExclusion values
                    while(true) {
                        nIndex = oSOMExprs.indexOf(',', nPrevIndex + 1);
                        if(nIndex == -1)
                            break;

                        oFieldStatus = oSOMExprs.substring(nPrevIndex + 1, nIndex);
                        nIndexOfSeparator = oFieldStatus.indexOf(oSeparator);
                        oSOMExpr = "data[0]." + oFieldStatus.substring(0, nIndexOfSeparator);
                        bStatus = oFieldStatus.substring(nIndexOfSeparator + 1);
                        oStatus = "visible";

                        if (bStatus == "true")
                            oStatus = "hidden";

                        oFields[oSOMExpr] = oStatus;
                        nPrevIndex = nIndex;
                    }
                    oFieldStatus = oSOMExprs.substring(nPrevIndex + 1);
                    nIndexOfSeparator = oFieldStatus.indexOf(oSeparator);
                    oSOMExpr = "data[0]." + oFieldStatus.substring(0, nIndexOfSeparator);
                    bStatus = oFieldStatus.substring(nIndexOfSeparator + 1);
                    oStatus = "visible";

                    if (bStatus == "true")
                        oStatus = "hidden";

                    oFields[oSOMExpr] = oStatus;

                    // get objects corresponding to templateIds
                    iterate(xfa.form, oFields);
              }

              /**
               * Maps SOM Expression or templateId (backward compatibility) to corresponding form field object.
               */
              function iterate(oTreeNode, oFields) {
                var somExpr = oTreeNode.somExpression.substring(xfa.form.somExpression.length + 1);
                if (oFields.hasOwnProperty(somExpr))
                     oTreeNode.presence = oFields[somExpr];
                else if(oFields.hasOwnProperty(oTreeNode.id)) // if we get templateId (backward compatibility)
                     oTreeNode.presence = oFields[oTreeNode.id];

                // iterate through children of subform and form only
                if(oTreeNode.className == "subform" || oTreeNode.className == "form") {
                    var oNodes = oTreeNode.nodes;
                    for (var nCount = 0; nCount &lt; oNodes.length; nCount++) {
                        iterate(oNodes.item(nCount), oFields);
                    }
                }
            }
         </script>
            <script contentType="application/x-javascript" name="dorTableRowShading" runAt="both">
            function shadeRows(oTableRow) {
               applyRowShading(oTableRow);
            }

            /**
             * If apply flag in row shading instruction is 1 extract values of first color
             * and next color and use them to alternately shade rows
             */
            function applyRowShading(oTableRow) {
               // get xml of table tag
               var oSomExp = oTableRow.parent.somExpression.substring(xfa.form.somExpression.length + 1);
               var oParentXML = xfa.template.resolveNode(oSomExp).saveXML("pretty");
               // get regular expression to extract row shading instruction
               var oFind = new RegExp(dorRowShadingInst.value, "g");
               // get string for row shading instruction
               var oRowShadingInst = oFind.exec(oParentXML)[0];

               // regular expression to get color values and appy flag
               var oColorRegExp = new RegExp("firstcolor:.*", "g");
               // get string for color values and apply flag
               var oColors = oColorRegExp.exec(oRowShadingInst)[0];

               // values are separated by ,
               var nIndexOfComma = oColors.indexOf(',');
               // get value for first color
               var oFirstColor = oColors.substring(11, nIndexOfComma);
               nIndexOfComma = oColors.indexOf(',', nIndexOfComma + 1);
               // get value for next color
               var oNextColor = oColors.substring(11 + oFirstColor.length + 2 + 10, nIndexOfComma);
               // get value of apply flag
               var oApply = oColors.substring(11 + oFirstColor.length + 2 + 10 + oNextColor.length + 2 + 6);

               // if apply flag is 1 only then apply row shading
               if (oApply === '1')
                  shadeRowsWithColor(oTableRow, oFirstColor, oNextColor);
            }

            /**
             * Actual function to shade rows of the table alternately.
             * @param oTableRow table row which has to be shade
             * @param oFirstColor color with which first row would be shade
             * @param oNextColor color with which next row would be shade
             */
            function shadeRowsWithColor(oTableRow, oFirstColor, oNextColor) {
               // get hexadecimal values for colors
               oFirstColor = getHexColor(oFirstColor);
               oNextColor = getHexColor(oNextColor);

               // get colors in string format
               var oColor1 = getColorString(oFirstColor);
               var oColor2 = getColorString(oNextColor);

               // shade rows alternately
               var oFreqs=[1,1];
               var oColors=[oColor1, oColor2];
               var nFreqSize = 0;

               for (var i = 0; i &lt; oFreqs.length; i++)
                  nFreqSize += oFreqs[i];

               var nRelIndex = oTableRow.index % nFreqSize + 1;

               for (var nRun=0; nRun &lt; oFreqs.length; nRun++) {
                  if (nRelIndex &gt; oFreqs[nRun]) {
                     nRelIndex -= oFreqs[nRun];
                  }
                  else {
                     oTableRow.border.fill.color.value = oColors[nRun];
                     break;
                  }
               }
            }

            /**
             * Function that returns six digit hexadecimal value for color
             * @param oColor hexadecimal color value having six or less digits
             * @return six digit hexadecimal value with zero prefixed if needed
             */
            function getHexColor(oColor) {
               while (oColor.length &lt; 6) {
                  oColor = '0' + oColor;  // zero pad
               }
               return oColor;
            }


            /**
             * Function that returns color string corresponding to hexadecimal color value
             * @param oHexColor fixed length (6) hexadecimal color value
             * @return color string
             */
            function getColorString(oHexColor) {
               var oColor = parseInt(oHexColor.substring(0, 2), 16).toString() + ","
                              + parseInt(oHexColor.substring(2, 4), 16).toString() + ","
                              + parseInt(oHexColor.substring(4, 6), 16).toString();
               return oColor;
            }
</script>
            <script contentType="application/x-javascript" name="dorMergeElementOverriddenData">
            function mergeOverriddenData(stateOverrideNode) {
                        var elementJson = {};
                        var oNodes = stateOverrideNode.nodes;
                        for (var nCount = 0; nCount &lt; oNodes.length; nCount++) {
                            var node = oNodes.item(nCount);
                            if (node.className === "subform") {
                            var overriddenElement = getElementJson(node);
                            if (overriddenElement) {
                                elementJson[overriddenElement.somExp] = overriddenElement;
                            }
                    }
                }
                iterate(xfa.form, elementJson);
            }

            function getElementJson(elementNode) {
                    var somNode = elementNode.resolveNode("AF_SOMEXPR_XFO");
                if (somNode  &amp;&amp; somNode.rawValue) {
                    var retvalMap = {"somExp": somNode.rawValue};
                    var optionsNode = elementNode.resolveNode("AF_OPTIONS_XFO");
                    if (optionsNode) {
                            var optionsMap = getOptionsMap(optionsNode);
                            if (optionsMap) {
                                retvalMap.options = optionsMap;
                            }
                    }
                    return retvalMap;
                }
            }

            function getOptionsMap(optionsNode) {
                var retvalMap;
                var oNodes = optionsNode.nodes;
                for (var nCount = 0; nCount &lt; oNodes.length; nCount++) {
                    var optionNode = oNodes.item(nCount);
                    if (optionNode != null &amp;&amp; optionNode.className === "subform") {
                            var optionDisplayNode = optionNode.resolveNode("AF_DISPLAY_XFO");
                         var optionSaveNode = optionNode.resolveNode("AF_SAVE_XFO");
                            if (optionDisplayNode &amp;&amp; optionDisplayNode.rawValue &amp;&amp; optionSaveNode &amp;&amp; optionSaveNode.rawValue) {
                                retvalMap = retvalMap || {};
                                retvalMap[optionSaveNode.rawValue] = optionDisplayNode.rawValue
                        }
                    }
                }
                return retvalMap;
            }

            /**
             * Maps SOM Expression or templateId (backward compatibility) to corresponding form field object.
             */
            function iterate(oTreeNode, elementJson) {
                var somExpr = oTreeNode.somExpression.substring(xfa.form.data.somExpression.length + 1);
                if (elementJson.hasOwnProperty(somExpr)) {
                    dorDefaultElementOverrideHandler.overrideOptionsForElement(oTreeNode, elementJson);
                } else if (elementJson.hasOwnProperty(oTreeNode.id)) { // if we get templateId (backward compatibility)
                    dorDefaultElementOverrideHandler.overrideOptionsForElement(oTreeNode, elementJson);
                }
                // iterate through children of subform and form only
                if (oTreeNode.className == "subform" || oTreeNode.className == "form") {
                    var oNodes = oTreeNode.nodes;
                    for (var nCount = 0; nCount &lt; oNodes.length; nCount++) {
                        iterate(oNodes.item(nCount), elementJson);
                    }
                }
            }

             function isFieldOverridden(field) {
                var somExprRequired = field.somExpression.substring(xfa.form.data.somExpression.length + 1);
                var stateOverrideNode = xfa.resolveNode("AF_STATEOVERRIDESSUBFORM_XFO");
                var oNodes = stateOverrideNode.nodes;
                for (var nCount = 0; nCount &lt; oNodes.length; nCount++) {
                    var node = oNodes.item(nCount);
                    if (node.className === "subform") {
                        var overriddenElement = getElementJson(node);
                        if (overriddenElement &amp;&amp; overriddenElement.somExp === somExprRequired) {
                            return true;
                        }
                    }
                }
                return false;
            }

</script>
            <script contentType="application/x-javascript" name="dorDefaultElementOverrideHandler"> function overrideOptionsForElement(elementNode, fieldOverriddenData) {
    var element = elementNode.resolveNode("extras.dorElementType");
    if (element != null) {
        var elementType = element.value;
        if (elementType === 'AF_CHECKBOX_XFO') {
            dorCheckBoxMerge.overrideOptionsForCheckBox(elementNode, fieldOverriddenData);
        } else if (elementType === 'AF_RADIOBUTTONLIST_XFO') {
            dorRadioButtonMerge.overrideOptionsForRadioButton(elementNode, fieldOverriddenData);
        } else if (elementType === "AF_IMAGECHOICE_XFO") {
            dorCheckBoxMerge.overrideOptionsForImageChoice(elementNode, fieldOverriddenData);
        } else if (elementType === "AF_DROPDOWNLIST_XFO") {
            dorDropDownElementOverrideHandler.overrideOptionsForDropDown(elementNode, fieldOverriddenData);
        }
    }
 }

</script>
            <script contentType="application/x-javascript" name="dorDropDownElementOverrideHandler">
 function overrideOptionsForDropDown(field,fieldOverriddenData) {
    var somExpr = field.somExpression.substring(xfa.form.data.somExpression.length + 1);
    var elementOverrideData = fieldOverriddenData[somExpr];
    if (elementOverrideData.hasOwnProperty("options")) {
        if (checkIfNewDropDownValueSelected(field.rawValue,elementOverrideData.options)) {
            field.clearItems();
            for(var propertyName in elementOverrideData.options) {
                var value = elementOverrideData.options[propertyName];
                field.addItem(value, propertyName);
            }
        }
    }
 }

 function checkIfNewDropDownValueSelected(fieldValue,optionsMap) {
    var returnValue = false;
    for(var propertyName in optionsMap) {
        returnValue = returnValue  || (propertyName == fieldValue)
    }
    return returnValue;
 }
</script>
